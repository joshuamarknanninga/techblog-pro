{{!-- home.handlebars --}}
{{!-- Make sure data is passed in the correct format from the controller --}}
<div class="blog-container">
  {{#each posts}}
    <div class="post">
      <h2>{{this.title}}</h2>
      <p>{{this.content}}</p>
      <p>By {{this.User.name}}</p>
    </div>
  {{/each}}
</div>
